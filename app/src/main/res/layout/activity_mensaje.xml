<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Activity.Mensaje">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="251dp"
        android:layout_height="121dp"
        android:layout_marginTop="172dp"
        android:text="Para que pueda acceder a su perfil primero debe terminar de completarlo para que asi pueda ser visible solo le tomara unos minutos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/aceptar_mensaje"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:layout_marginTop="112dp"

        android:background="@drawable/boton_pres1"

        android:text="Aceptar"
        android:textColor="#000"
        app:backgroundTint="@color/white"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />
</androidx.constraintlayout.widget.ConstraintLayout>